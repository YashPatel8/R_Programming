{
if(K31[i] <= X32[i])
{
R[i] <- runif(1,0,1)
if(R[i] > 0.5)
{
Y31[i] <- X31[i]
}
if(R[i] <= 0.5)
{
Y31[i] <- -X31[i]
}
}
}
print(R)
print(Y31)
newR <- na.omit(R)
print(newR)
